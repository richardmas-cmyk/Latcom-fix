# Environment Configuration
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/latcom

# Test Mode
TEST_MODE=false
MOCK_PROVIDERS=false

# Customer Credentials
ENVIADESPENSA_API_KEY=your_enviadespensa_key_here
ENVIADESPENSA_CLIENT_ID=your_client_id_here

# Latcom Provider
LATCOM_BASE_URL=https://api.latcom.com
LATCOM_API_KEY=your_latcom_api_key
LATCOM_DISTRIBUTOR_ID=your_distributor_id
LATCOM_MODE=XOOM_ONLY

# PPN Provider
PPN_BASE_URL=https://api.ppn.com
PPN_API_KEY=your_ppn_api_key
PPN_MERCHANT_ID=your_merchant_id

# CSQ Provider
# Development: DEVELOPUS / n7ive4i9ye / 173103
# Production: (get from CSQ)
CSQ_BASE_URL=https://evsbus.csqworld.com
CSQ_USERNAME=your_username
CSQ_PASSWORD=your_password
CSQ_TERMINAL_ID=your_terminal_id
CSQ_DEFAULT_OPERATOR_ID=1

# MUWE Provider
MUWE_BASE_URL=https://api.muwe.com
MUWE_API_KEY=your_muwe_api_key
MUWE_MERCHANT_ID=your_merchant_id

# Telefonica Mexico Provider (Movistar)
TELEFONICA_WSDL_URL=http://srm.movistar.com.mx/services/RecargaServicioPort?wsdl
TELEFONICA_PASSWORD_IVR=your_password_ivr_here
TELEFONICA_PUNTO_VENTA=your_distributor_id_here
TELEFONICA_DEFAULT_TYPE=00

# Admin Keys
ADMIN_KEY=your_admin_key_here
RECONCILE_KEY=your_reconcile_key_here

# Forex API
FOREX_API_KEY=your_forex_api_key

# Alert System
ALERT_WEBHOOK_URL=your_webhook_url
ENABLE_ALERTS=true

# Redis (for queue management)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info
